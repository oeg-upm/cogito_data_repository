shape:Image
    a               sh:NodeShape ;
    rdfs:label      "Image" ;
    sh:description  "A picture of an entity" ;
    sh:name         "Image" ;
    sh:nodeKind     sh:IRI ;
    sh:property     shape:isRelatedToJob , shape:hasFileFormat , shape:hasFileURL ;
    sh:pattern      "http://data.cogito.iot.linkeddata.es/resources/image/" ;
    sh:targetClass  qual:Image .